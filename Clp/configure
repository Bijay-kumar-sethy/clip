$as_echo "$as_me: Glpk DEPENDENCIES are $GLPK_DEPENDENCIES" >&6;}
    fi
    if test -n "$GLPK_DATA" ; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: Glpk DATA   is  $GLPK_DATA" >&5
$as_echo "$as_me: Glpk DATA   is  $GLPK_DATA" >&6;}
    fi
    if test -n "$GLPK_PCLIBS" ; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: Glpk PCLIBS are $GLPK_PCLIBS" >&5
$as_echo "$as_me: Glpk PCLIBS are $GLPK_PCLIBS" >&6;}
    fi
    if test -n "$GLPK_PCREQUIRES" ; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: Glpk PCREQUIRES are $GLPK_PCREQUIRES" >&5
$as_echo "$as_me: Glpk PCREQUIRES are $GLPK_PCREQUIRES" >&6;}
    fi

      { $as_echo "$as_me:${as_lineno-$LINENO}: ClpLib CFLAGS are $CLPLIB_CFLAGS" >&5
$as_echo "$as_me: ClpLib CFLAGS are $CLPLIB_CFLAGS" >&6;}
      { $as_echo "$as_me:${as_lineno-$LINENO}: ClpLib LIBS   are $CLPLIB_LIBS" >&5
$as_echo "$as_me: ClpLib LIBS   are $CLPLIB_LIBS" >&6;}
      { $as_echo "$as_me:${as_lineno-$LINENO}: ClpLib DEPENDENCIES are $CLPLIB_DEPENDENCIES" >&5
$as_echo "$as_me: ClpLib DEPENDENCIES are $CLPLIB_DEPENDENCIES" >&6;}

  fi
fi

