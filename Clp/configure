      { $as_echo "$as_me:${as_lineno-$LINENO}: OsiClpLib CFLAGS are $OSICLPLIB_CFLAGS" >&5
$as_echo "$as_me: OsiClpLib CFLAGS are $OSICLPLIB_CFLAGS" >&6;}
